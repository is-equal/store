{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "moduleResolution": "Node",
    "strict": true,
    "declaration": true,
    "stripInternal": true,
    "skipLibCheck": false,
    "baseUrl": ".",
    "paths": {
      "@equal/store": ["packages/store/src/index.ts"],
      "@equal/react-store": ["packages/react-store/src/index.ts"]
    }
  }
}
